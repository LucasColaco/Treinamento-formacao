<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <changeSet id="20210719174900-1" author="lucas.colaco">
        <createTable tableName="TB_RESPONSAVEL">
            <column name="ID" type="NUMBER(10)"></column>
            <column name="NOME" type="VARCHAR2(255)"></column>
            <column name="EMAIL" type="VARCHAR2(255)"></column>
            <column name="DT_NASCIMENTO" type="DATE"></column>
        </createTable>
    </changeSet>

    <changeSet id="20210719174900-2" author="lucas.colaco">
        <addNotNullConstraint tableName="TB_RESPONSAVEL" columnName="ID"/>
        <addNotNullConstraint tableName="TB_RESPONSAVEL" columnName="NOME"/>
        <addNotNullConstraint tableName="TB_RESPONSAVEL" columnName="EMAIL"/>
        <addNotNullConstraint tableName="TB_RESPONSAVEL" columnName="DT_NASCIMENTO"/>
    </changeSet>

    <changeSet id="20210719174900-3" author="lucas.colaco">
        <addPrimaryKey tableName="TB_RESPONSAVEL" columnNames="ID" constraintName="RESPONSAVEL_PK"/>
    </changeSet>

    <changeSet id="20210719174900-4" author="lucas.colaco">
        <createSequence sequenceName="SEQ_RESPONSAVEL" incrementBy="1" startValue="1"/>
    </changeSet>
</databaseChangeLog>